{
	"title": "hyc_notes",
	"links": {
        "sidebar": {
            "Home": "https://hycpexm.github.io",
			"hycPEXM Github": "https://github.com/hycPEXM"
        }
    },
	"author":"hyc",
	"plugins" : [
		"expandable-chapters",
		"copy-code-button",
		"github-buttons",
		"page-footer-ex",
		"anchor-navigation-ex",
		"expandable-chapters-small",
		"prism", 
		"-highlight",
		"-lunr", 
		"-search", 
		"search-pro",
		"splitter",
		"image-captions",
		"katex-plus"
	],
	"pluginsConfig": {	
		"github-buttons": {
			"buttons": [{
				"user": "hycPEXM",
				"repo": "",
				"type": "star",
				"size": "small"
			}]
		},
		"page-footer-ex": {
            "copyright": "使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
            "markdown": true,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
		},
			
		"image-captions": {
          "caption": "Image _PAGE_IMAGE_NUMBER_ : _CAPTION_"
		},
		
		"prism": {
			"css": ["prismjs/themes/prism-solarizedlight.css"],
			"lang": {"flow": "typescript"}
		},
		
		"anchor-navigation-ex": {
		"showLevel": true,
		"associatedWithSummary": true,
		"multipleH1": false
		},
		
		"fontsettings": { 
            "size": 3        
        },
		
		"theme-default": {
            "showLevel": false
        }
		
	},	
	"ignores" : ["_book", "node_modules"]
}